# description	: The Private YouTube Client
# homepage	: https://freetubeapp.io/
# depends	: 

name=freetube
version=0.16.0
release=1
source="https://github.com/FreeTubeApp/FreeTube/releases/download/v0.16.0-beta/freetube_${version}_amd64.deb"

build() {
	bsdtar -xf freetube_${version}_amd64.deb
	tar -xf data.tar.xz -C $PKG
}
