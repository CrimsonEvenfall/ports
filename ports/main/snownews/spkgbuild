# description :  snownews 
# depends	: ncurses libxml2 libcurl gettext openssl lynx

name=snownews
version=1.9
release=1
source="https://github.com/msharov/$name/archive/refs/tags/v$version.tar.gz"

build() {
    cd $name-$version
    ./configure --prefix=/usr
	make
	make PREFIX=/usr DESTDIR=$PKG install
}
